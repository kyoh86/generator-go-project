{
  "name": "generator-go-project",
  "version": "1.1.0",
  "description": "A Yeoman generator to scaffold golang project.",
  "homepage": "https://github.com/kyoh86/generator-go-project",
  "author": {
    "name": "Kyoichiro Yamada",
    "email": "chili.pepper86@gmail.com",
    "url": "https://kyoh.net"
  },
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "keywords": [
    "Yeoman",
    "go",
    "golang",
    "Yeoman-generator",
    "generator",
    "scaffold"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "coveralls": "^3.0.0",
    "nsp": "^3.2.1",
    "eslint": "^4.1.0",
    "prettier": "^1.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-config-xo": "^0.20.1",
    "jest": "^22.0.6"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "ejs": "^2.5.8",
    "fuse.js": "^3.2.0",
    "inquirer-autocomplete-prompt": "^0.12.2",
    "mkdirp": "^0.5.1",
    "request": "^2.85.0",
    "spdx-license-list": "^4.0.0",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepare": "nsp check",
    "prepublishOnly": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "kyoh86/generator-go-project",
  "license": "MIT"
}
