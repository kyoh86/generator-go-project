{
  "name": "generator-go-project",
  "version": "1.4.6",
  "description": "A Yeoman generator to scaffold golang project.",
  "homepage": "https://github.com/kyoh86/generator-go-project",
  "author": {
    "name": "Kyoichiro Yamada",
    "email": "chili.pepper86@gmail.com",
    "url": "https://kyoh.net"
  },
  "files": [
    "generators",
    "lib"
  ],
  "main": "generators/app/index.js",
  "keywords": [
    "Yeoman",
    "go",
    "golang",
    "Yeoman-generator",
    "generator",
    "scaffold"
  ],
  "devDependencies": {
    "coveralls": "^3.0.11",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-xo": "^0.29.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^2.4.1"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "ejs": "^3.0.2",
    "fuse.js": "^5.1.0",
    "inquirer-autocomplete-prompt": "^1.0.2",
    "mkdirp": "^1.0.4",
    "request": "^2.88.2",
    "spdx-license-list": "^6.1.0",
    "yeoman-generator": "^4.8.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepare": "yarn audit",
    "prepublishOnly": "yarn audit",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "kyoh86/generator-go-project",
  "license": "MIT"
}
